{
    "name": "tailwind-starterkit",
    "version": "1.0.0",
    "description": "tailwind-starterkit",
    "scripts": {
        "start": "tailwind build ./src/Tailwind/index.css -o ./src/Tailwind/index.css && webpack serve --config webpack.dev.js ",
        "build": "rm -rf dist && tailwind build ./src/Tailwind/index.css -o ./src/Tailwind/index.css && webpack --config webpack.prod.js",
        "test": "jest --watch /tests",
        "server": "rm -rf dist && webpack --config webpack.prod.js && node server.js"
    },
